<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.mskcc.cbio.portal.persistence.GeneMapper">
    
<cache/>

<select id="byHugoGeneSymbol" resultType="DBGene" parameterType="list">
    select
        HUGO_GENE_SYMBOL as hugoGeneSymbol,
        ENTREZ_GENE_ID as entrezGeneId
    from gene
    where HUGO_GENE_SYMBOL in
    <foreach item="item" collection="ids" open="(" separator="," close=")">
        #{item}
    </foreach>
</select>
<select id="byEntrezGeneId" resultType="DBGene" parameterType="list">
    select
        HUGO_GENE_SYMBOL as hugoGeneSymbol,
        ENTREZ_GENE_ID as entrezGeneId
    from gene
    where ENTREZ_GENE_ID in
    <foreach item="item" collection="ids" open="(" separator="," close=")">
        #{item}
    </foreach>
</select> 
    
</mapper>