<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.mskcc.cbio.portal.persistence.GDDMapper">

<cache/>

<insert id="insertGddData" parameterType="map" useGeneratedKeys="true" keyProperty="internal_id" keyColumn="internal_id">
    insert into gdd (INTERNAL_ID, GDD_DATA)
    values (#{internal_id},#{gdd_data})
</insert>

<select id="getGddData" resultType="String">
    select 
        gdd.GDD_DATA
    from gdd
    where gdd.INTERNAL_ID in <foreach item="item" collection="internal_ids" open="(" separator="," close=")">#{item}</foreach>
    
</select>

</mapper>