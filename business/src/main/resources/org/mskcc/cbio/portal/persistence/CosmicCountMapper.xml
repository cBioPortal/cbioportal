<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.mskcc.cbio.portal.persistence.CosmicCountMapper">

<select id="getCosmicCountsByKeyword" resultType="DBCosmicCount">
    select
        KEYWORD as keyword,
        COUNT as count,
        PROTEIN_CHANGE as protein_change
    from cosmic_mutation
    where KEYWORD in <foreach item="item" collection="keywords" open="(" separator="," close=")"> #{item} </foreach>
</select>
</mapper>