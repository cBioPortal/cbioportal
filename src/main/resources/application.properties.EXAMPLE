# app name
app.name=cbioportal

# Spring Boot Properties 2.7.14
spring.mvc.pathmatch.matching-strategy = ANT_PATH_MATCHER

#Clickhouse Enabled
# Set to True to enable Clickhouse (Warning Experimental Features)
